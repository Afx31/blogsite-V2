generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider      = "mysql"
  url           = env("DATABASE_URL")
  relationMode  = "prisma"
}

model Post {
  id          Int       @id @default(autoincrement())
  createdAt   DateTime  @default(now())
  title       String
  slug        String
  car         String
  thumbnail   String
  description String
  content     String?   @db.VarChar(14000)
}
